{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["README.md", "./src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|\\.test\\.js$|(^|\\/|\\\\)_)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "docdash": {
    "sort": true,
    "search": true,
    "openGraph": {
      "title": "Flynamo - Let your AWS DynamoDB client take off!",
      "type": "website",
      "site_name": "Flynamo",
      "url": "https://flybondi.github.io/flynamo/"
    }
  },
  "sourceType": "module"
}
